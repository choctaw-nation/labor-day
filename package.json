{
	"name": "cno-labor-day",
	"version": "1.9.0",
	"description": "Custom WP theme for CNO labor day site",
	"main": "wp-content/themes/labor-day/src/index.js",
	"prettier": "@wordpress/prettier-config",
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"scripts": {
		"start": "wp-scripts start",
		"build": "wp-scripts build",
		"format": "wp-scripts format ./wp-content/themes/labor-day/src/*",
		"lint:style": "wp-scripts lint-style './wp-content/themes/labor-day/src/**/*.scss'",
		"lint:js": "wp-scripts lint-js './wp-content/themes/labor-day/src/js/*.js'",
		"lint:jsx": "wp-scripts lint-js './wp-content/themes/labor-day/src/**/*.jsx'",
		"lint:ts": "wp-scripts lint-js './wp-content/themes/labor-day/src/**/*.ts'",
		"lint:tsx": "wp-scripts lint-js './wp-content/themes/labor-day/src/**/*.tsx'",
		"lint:scripts": "npm-run-all lint:js lint:jsx lint:ts lint:tsx",
		"packages-update": "wp-scripts packages-update"
	},
	"author": "Choctaw Nation of Oklahoma",
	"license": "ISC",
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^6.4.2",
		"@fortawesome/free-brands-svg-icons": "^6.4.2",
		"@fortawesome/free-regular-svg-icons": "^6.4.2",
		"@fortawesome/free-solid-svg-icons": "^6.4.2",
		"@fortawesome/react-fontawesome": "^0.2.0",
		"@wordpress/scripts": "^26.9.0",
		"aos": "^2.3.4",
		"bootstrap": "^5.3.1",
		"date-fns": "^2.30.0",
		"fuse.js": "^6.6.2",
		"swiper": "^10.1.0"
	},
	"devDependencies": {
		"@fortawesome/fontawesome-free": "^6.4.2",
		"@loadingio/css-spinner": "^2.0.2",
		"@types/aos": "^3.0.4",
		"@wordpress/prettier-config": "^2.21.0",
		"animate.css": "^4.1.1",
		"eslint-config-react-app": "^7.0.1",
		"npm-run-all": "^4.1.5"
	}
}
