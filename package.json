{
  "name": "cno-labor-day",
  "version": "1.0.0",
  "description": "Custom WP theme for CNO labor day site",
  "main": "wp-content/themes/labor-day/src/index.js",
  "scripts": {
    "start": "wp-scripts start",
    "build": "wp-scripts build",
    "format": "wp-scripts format ./wp-content/themes/labor-day/src/*",
    "lint:style": "wp-scripts lint-style ./wp-content/themes/labor-day/src/styles/**/*.scss",
    "fix:style": "wp-scripts lint-style ./wp-content/themes/labor-day/src/styles/**/*.scss --fix",
    "lint:js": "wp-scripts lint-js ./wp-content/themes/labor-day/src/js/**/*.js",
    "lint:md:docs": "wp-scripts lint-md-docs",
    "lint:pkg-json": "wp-scripts lint-pkg-json",
    "packages-update": "wp-scripts packages-update",
    "plugin-zip": "wp-scripts plugin-zip",
    "test:e2e": "wp-scripts test-e2e",
    "test:unit": "wp-scripts test-unit-js"
  },
  "author": "Choctaw Nation of Oklahoma",
  "license": "ISC",
  "dependencies": {
    "@wordpress/scripts": "^26.2.0",
    "bootstrap": "^5.2.3",
    "date-fns": "^2.30.0",
    "fuse.js": "^6.6.2",
    "swiper": "^9.2.2"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@loadingio/css-spinner": "^2.0.2",
    "webpack-bundle-analyzer": "^4.8.0"
  }
}