{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"palette": [
				{
					"color": "#7d8034",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#50551d",
					"name": "Primary (Dark)",
					"slug": "primary-dark"
				},
				{
					"color": "#b0b700",
					"name": "Primary (Light)",
					"slug": "primary-light"
				},
				{
					"color": "#f09416",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#8c52c1",
					"name": "Accent",
					"slug": "accent"
				},
				{
					"color": "#efe6d9",
					"name": "Light",
					"slug": "light"
				},
				{
					"color": "#777",
					"name": "Gray",
					"slug": "gray"
				},
				{
					"color": "white",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#000000",
					"name": "Black",
					"slug": "black"
				}
			]
		},
		"blocks": {
			"core/group": {
				"background": {
					"backgroundImage": true,
					"backgroundSize": true
				}
			}
		},
		"dimensions": {
			"aspectRatio": true
		},
		"layout": {
			"allowCustomContentAndWideSize": false,
			"allowEditing": true,
			"wideSize": "1320px",
			"contentSize": "768px"
		},
		"shadow": {
			"defaultPresets": false
		},
		"spacing": {
			"blockGap": true,
			"customSpacingSize": false,
			"defaultSpacingSizes": false,
			"margin": true,
			"padding": true,
			"spacingSizes": [
				{
					"name": "None",
					"size": "0",
					"slug": "none"
				},
				{
					"name": "Extra Small",
					"size": ".25rem",
					"slug": "xs"
				},
				{
					"name": "Small",
					"size": ".5rem",
					"slug": "sm"
				},
				{
					"name": "Base",
					"size": "1rem",
					"slug": "base"
				},
				{
					"name": "Large",
					"size": "1.5rem",
					"slug": "lg"
				},
				{
					"name": "Extra Large",
					"size": "3rem",
					"slug": "xl"
				}
			]
		},
		"typography": {
			"customFontSize": false,
			"defaultFontSizes": false,
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "gill-sans-nova, \"Helvetica Neue\", \"Noto Sans\", \"Liberation Sans\", Arial, sans-serif, \"Apple Color Emoji\", \"Segoe UI Emoji\", \"Segoe UI Symbol\", \"Noto Color Emoji\"",
					"name": "Gill Sans Nova",
					"slug": "body"
				}
			],
			"fontSizes": [
				{
					"slug": "root",
					"size": "1rem",
					"name": "Root",
					"fluid": false
				},
				{
					"slug": "base",
					"size": "1rem",
					"name": "Body",
					"fluid": {
						"min": "1rem",
						"max": "1.25rem"
					}
				},
				{
					"slug": "sm",
					"size": "1.2rem",
					"name": "Small (H6)",
					"fluid": {
						"min": "1.2rem",
						"max": "1.5rem"
					}
				},
				{
					"slug": "md",
					"size": "1.44rem",
					"name": "Medium (H5)",
					"fluid": {
						"min": "1.44rem",
						"max": "1.88rem"
					}
				},
				{
					"slug": "lg",
					"size": "1.728rem",
					"name": "Large (H4)",
					"fluid": {
						"min": "1.728rem",
						"max": "2.16rem"
					}
				},
				{
					"slug": "xl",
					"size": "2.074rem",
					"name": "XL (H3)",
					"fluid": {
						"min": "2.074rem",
						"max": "2.592rem"
					}
				},
				{
					"slug": "xxl",
					"size": "2.488rem",
					"name": "2XL (H2)",
					"fluid": {
						"min": "2.488rem",
						"max": "3.11rem"
					}
				},
				{
					"slug": "xxxl",
					"size": "2.986rem",
					"name": "3XL (H1)",
					"fluid": {
						"min": "2.986rem",
						"max": "3.73rem"
					}
				}
			],
			"fontWeight": true,
			"lineHeight": false,
			"textDecoration": false
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"spacing": {
			"padding": {
				"bottom": "0",
				"left": "var:preset|spacing|lg",
				"right": "var:preset|spacing|lg",
				"top": "0"
			},
			"blockGap": "var:preset|spacing|base"
		},
		"blocks": {
			"core/heading": {
				"typography": {
					"fontFamily": "var:preset|font-family|body"
				}
			},
			"core/list": {
				"typography": {
					"fontFamily": "var:preset|font-family|body",
					"fontSize": "var:preset|font-size|base"
				}
			},
			"core/list-item": {
				"typography": {
					"fontFamily": "var:preset|font-family|body",
					"fontSize": "var:preset|font-size|base"
				}
			},
			"core/paragraph": {
				"typography": {
					"fontFamily": "var:preset|font-family|body",
					"fontSize": "var:preset|font-size|base"
				}
			}
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var:preset|font-size|xxxl"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var:preset|font-size|xxl"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var:preset|font-size|xl"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var:preset|font-size|lg"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var:preset|font-size|md"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var:preset|font-size|sm"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var:preset|font-family|body"
				}
			}
		},
		"typography": {
			"fontSize": "var:preset|font-size|base"
		}
	}
}
