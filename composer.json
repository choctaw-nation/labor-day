{
	"name": "choctaw-nation/labor-day",
	"description": "a custom wp theme for Choctaw Nation's labor day site",
	"type": "project",
	"repositories": [
		{
			"type": "composer",
			"url": "https://connect.advancedcustomfields.com"
		}
	],
	"extra": {
		"installer-paths": {
			"wp-content/mu-plugins/{$name}/": [
				"wpengine/advanced-custom-fields-pro"
			]
		}
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"require": {
		"wpengine/advanced-custom-fields-pro": "^6.1.6",
		"wp-graphql/wp-graphql-acf": "^0.6.1",
		"dannyvanholten/acf-gravityforms-add-on": "^1.3"
	},
	"require-dev": {
		"wp-coding-standards/wpcs": "^2.3",
		"squizlabs/php_codesniffer": "^3.7.2"
	}
}